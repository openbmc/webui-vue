{
  "name": "webui-vue",
  "version": "0.1.0",
  "private": true,
  "description": "OpenBMC Web UI using the Vue.js front-end framework",
  "scripts": {
    "serve": "check-node-version --package && vue-cli-service serve",
    "build": "check-node-version --package && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:debugger": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "test:update": "vue-cli-service test:unit -u",
    "lint": "vue-cli-service lint",
    "docs:build": "vuepress build docs",
    "docs:serve": "vuepress dev docs",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "@carbon/icons-vue": "10.49.1",
    "@novnc/novnc": "1.3.0",
    "@vue/vue3-jest": "27.0.0",
    "axios": "0.27.2",
    "babel-eslint": "10.1.0",
    "bootstrap": "5.1.3",
    "bootstrap-vue": "2.22.0",
    "core-js": "3.23.4",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.3.6",
    "js-cookie": "3.0.1",
    "lodash": "4.17.21",
    "vue": "3.2.37",
    "vue-i18n": "9.1.10",
    "vue-router": "4.1.2",
    "vuelidate": "0.7.7",
    "vuex": "4.0.2",
    "xterm": "4.19.0",
    "xterm-addon-attach": "0.6.0",
    "xterm-addon-fit": "0.5.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "@intlify/vue-i18n-loader": "4.2.0",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-plugin-router": "5.0.8",
    "@vue/cli-plugin-unit-jest": "5.0.8",
    "@vue/cli-plugin-vuex": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/test-utils": "2.0.2",
    "babel-plugin-transform-require-context": "0.1.1",
    "check-node-version": "4.2.1",
    "compression-webpack-plugin": "10.0.0",
    "eslint": "8.19.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.2.0",
    "eslint-scope": "7.1.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "sass": "1.53.0",
    "sass-loader": "13.0.2",
    "vue-cli-plugin-i18n": "2.3.1",
    "vue-template-compiler": "2.7.5",
    "vuepress": "1.9.7"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged && vue-cli-service test:unit"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
