{
    "name": "webui-vue",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "description": "OpenBMC Web UI using the Vue.js front-end framework",
    "scripts": {
        "dev": "vite --mode development",
        "serve": "vite --mode development",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint src --ext .js,.vue",
        "i18n:report": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/locales/**/*.json'",
        "docs:build": "vuepress build docs",
        "docs:serve": "vuepress dev docs"
    },
    "dependencies": {
        "@carbon/icons-vue": "10.49.1",
        "@novnc/novnc": "1.3.0",
        "@vuelidate/core": "2.0.3",
        "@vuelidate/validators": "2.0.4",
        "axios": "1.13.2",
        "axios-cache-interceptor": "1.11.2",
        "bootstrap": "5.3.8",
        "bootstrap-vue-next": "0.40.8",
        "date-fns": "2.28.0",
        "date-fns-tz": "1.3.6",
        "js-cookie": "3.0.1",
        "lodash": "4.17.21",
        "mitt": "3.0.1",
        "vue": "3.5.24",
        "vue-i18n": "11.2.8",
        "vue-router": "4.6.3",
        "vuex": "4.1.0",
        "@xterm/xterm": "6.0.0",
        "@xterm/addon-attach": "0.12.0",
        "@xterm/addon-fit": "0.11.0"
    },
    "devDependencies": {
        "@vitejs/plugin-basic-ssl": "2.1.3",
        "@vitejs/plugin-vue": "5.2.4",
        "@vue/compiler-sfc": "3.5.22",
        "@vue/eslint-config-prettier": "9.0.0",
        "@vue/test-utils": "2.4.5",
        "@vue/vue3-jest": "27.0.0",
        "eslint": "8.57.1",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-vue": "9.2.0",
        "lint-staged": "13.0.3",
        "prettier": "3.2.5",
        "sass": "1.97.2",
        "vite": "6.4.1",
        "vite-plugin-compression": "0.5.1",
        "vite-svg-loader": "5.1.0",
        "vue-i18n-extract": "2.0.7",
        "vuepress": "1.9.7"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "lint-staged": {
        "*.{js,vue}": [
            "eslint --fix",
            "git add"
        ]
    }
